/*
Write an application that includes the logic from Practical 1A, obtains two numbers from the user, and displays them, but rejects any input where both numbers are greater than 10 and asks for two new numbers.
*/

using System;
namespace practical3c
{
    class P3C
    {
        static void Main(string[] args)
        {
            int var1, var2;
            l1:    // Label Created
            Console.WriteLine("Enter Two No ");
            
            
            var1 = Convert.ToInt32(Console.ReadLine());
            var2 = Convert.ToInt32(Console.ReadLine());

            if (var1 > 10 && var2 > 10)
            {
                goto l1;
            }
            Console.WriteLine("Your First no = {0} and Second no = {1}",var1,var2);
            Console.ReadLine();

        }

    }
}
