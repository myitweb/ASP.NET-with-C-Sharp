/*
Write a program using Fibonacci series.
*/

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace bhavi1
{
    class Program
    {
        static void Main(string[] args)
        {
            int f1, f2, fib;
            f1 = 0;
            f2 = 1;
            fib = 0;
            Console.WriteLine("Enter the no:");
            int n = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine(f1 + "\n" + f2 + "\n");
            for (int i = 0; i < n; i++)
            {
                f1 = f2;
                f2 = fib;
                fib = f1 + f2;
                Console.WriteLine(fib);
            }

            Console.ReadKey();
        }
    }
}
