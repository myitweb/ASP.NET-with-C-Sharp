/*
Generate various patterns (triangles, diamond and other patterns) with numbers.
*/

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace practical_no._2_B_
{
    class Program
    {
        static void Main(string[] args)
        {
            int i, j;
            for (i = 0; i <= 4; i++)
            {
                for (j = 0; j <= i; j++)
                {
                    Console.Write("*");
                }
                Console.WriteLine();
            }
            Console.ReadKey();
        }
    }
}
