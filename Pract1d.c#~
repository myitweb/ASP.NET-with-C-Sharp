/* 
Write a console application that places double quotation marks around each word in a string.
*/

using System;
namespace practical3c
{
    class P3C
    {
        static void Main(string[] args)
        {
            int var1, var2;
            l1:    // Label Created
            Console.WriteLine("Enter Two No ");
            
            
            var1 = Convert.ToInt32(Console.ReadLine());
            var2 = Convert.ToInt32(Console.ReadLine());

            if (var1 > 10 && var2 > 10)
            {
                goto l1;
            }
            Console.WriteLine("Your First no = {0} and Second no = {1}",var1,var2);
            Console.ReadLine();

        }

    }
}
