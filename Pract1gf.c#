/* Write a program to create a Array foreach loop. */

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] a = new int[5];
            Console.WriteLine("enter No");
            for (int i = 0; i < a.Length; i++)
            {
               
                a[i] = Convert.ToInt32(Console.ReadLine());
            }
            foreach (int v in a)
            {
                Console.WriteLine("U hav entered No:" + v);
            }
            Console.ReadKey();
        }
    }
}

