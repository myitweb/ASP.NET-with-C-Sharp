/*
If you have two integers stored in variables var1 and var2, what Boolean test can you
perform to see if one or the other (but not both) is greater than 10?

*/

using System;
namespace Practical1B
{
    class P1B
    {
        static void Main(string[] args)
        {
            int var1, var2;
            Console.WriteLine("Enter Two No ");
            var1 = Convert.ToInt32(Console.ReadLine());
            var2 = Convert.ToInt32(Console.ReadLine());
            if (var1 >= 10 && var2 >= 10) 
            {
                Console.WriteLine("Invalid Input ");
            }
            else if (var1 <= 10 && var2 <= 10)
            {
                Console.WriteLine("Invalid Input ");
            }
            else if (var1 > 1)
            {
                Console.WriteLine("Valid Input ");
            }
            else if (var2 > 1)
            {
                Console.WriteLine("Valid Input ");
            }
               Console.ReadLine();
        
        }

    }
}
